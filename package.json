{
  "name": "@nodecfdi/cfdi-to-pdf",
  "version": "1.6.0",
  "description": "Librer√≠a para crear un pdf basado en un XML CFDI o Retenciones",
  "keywords": [
    "cfdi",
    "retenciones",
    "xml",
    "pdf"
  ],
  "private": false,
  "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cfdi-to-pdf.cjs",
  "module": "./dist/cfdi-to-pdf.js",
  "unpkg": "./dist/cfdi-to-pdf.global.js",
  "browser": "./dist/cfdi-to-pdf.global.js",
  "types": "./dist/cfdi-to-pdf.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/cfdi-to-pdf.d.ts",
        "default": "./dist/cfdi-to-pdf.js"
      },
      "require": {
        "types": "./dist/cfdi-to-pdf.d.cts",
        "default": "./dist/cfdi-to-pdf.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "lint": "xo --fix",
    "lint:check": "xo",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "rimraf dist && tsup",
    "gen:docs": "rimraf docs && typedoc --options typedoc.json",
    "release": "pnpm run build && pnpm changeset publish"
  },
  "repository": "https://github.com/nodecfdi/cfdi-to-pdf",
  "bugs": {
    "url": "https://github.com/nodecfdi/cfdi-to-pdf/issues"
  },
  "homepage": "https://github.com/nodecfdi/cfdi-to-pdf",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@microsoft/api-extractor": "^7.39.1",
    "@types/is-ci": "^3.0.4",
    "@vitest/coverage-istanbul": "^1.1.3",
    "@types/pdfmake": "^0.2.8",
    "@xmldom/xmldom": "^0.8.10",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^23.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.7",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "@nodecfdi/cfdi-expresiones": "^2.1.6",
    "@nodecfdi/cfdiutils-common": "^2.0.0",
    "pdfmake": "^0.2.9"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
